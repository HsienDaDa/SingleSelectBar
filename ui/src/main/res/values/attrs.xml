<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SingleSelectBar">
        <attr name="uiColorSelected" format="color"/>
        <attr name="uiColorUnselected" format="color"/>
        <attr name="uiTextAppearance" format="integer"/>
        <attr name="uiStrokeWidth" format="dimension"/>
        <attr name="uiPressedEffectMode">
            <enum name="none" value="0"/>
            <enum name="light" value="1"/>
            <enum name="dark" value="2"/>
        </attr>
    </declare-styleable>
</resources>